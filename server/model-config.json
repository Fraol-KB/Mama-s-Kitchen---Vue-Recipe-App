{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDB"
  },
  "AccessToken": {
    "dataSource": "mongoDB",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDB",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDB",
    "public": false
  },
  "Recipe": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Storage": {
    "dataSource": "files",
    "public": true
  },
  "comment": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Review": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Favorite": {
    "dataSource": "mongoDB",
    "public": true
  }
}
